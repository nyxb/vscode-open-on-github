{
  "name": "vscode-open-on-github",
  "displayName": "Open on GitHub",
  "version": "0.0.1",
  "description": "üîç Open the current project or file in üêô github.com.",
  "author": "Dennis Ollhoff <contact@nyxb.xyz>",
  "license": "MIT",
  "homepage": "https://üíªnyxb.ws",
  "publisher": "nyxb",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyxb/vscode-open-on-github.git"
  },
  "bugs": {
    "url": "https://github.com/nyxb/vscode-open-on-github/issues"
  },
  "main": "dist/extension.js",
  "activationEvents": [
    "onCommand:openOnGitHub.openProject",
    "onCommand:openOnGitHub.openFile",
    "onCommand:openOnGitHub.openFileBlame",
    "onCommand:openOnGitHub.openFileHistory",
    "onCommand:openOnGitHub.openFilePermalink",
    "onCommand:openOnGitHub.openIssues",
    "onCommand:openOnGitHub.openPullRequests",
    "onCommand:openOnGitHub.openReleases",
    "onCommand:openOnGitHub.openActions",
    "onCommand:openOnGitHub.openProjects",
    "onCommand:openOnGitHub.openWiki",
    "onCommand:openOnGitHub.openSettings",
    "onCommand:openOnGitHub.copyFilePermalink"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Open on GitHub - Configuration",
      "properties": {
        "openInGitHub.github.domain": {
          "type": "string",
          "description": "Custom GitHub domain",
          "default": "github.com"
        },
        "openInGitHub.remote.name": {
          "type": "string",
          "description": "Name of the remote repository",
          "default": "origin"
        },
        "openInGitHub.remote.branch": {
          "type": "string",
          "description": "Name of the remote branch",
          "default": "master"
        },
        "openInGitHub.useLocalBranch": {
          "type": "boolean",
          "description": "Use the local branch instead of the fixed remote branch",
          "default": true
        },
        "openInGitHub.useLocalRange": {
          "type": "boolean",
          "description": "Highlight the local selection range, if there's one",
          "default": true
        },
        "openInGitHub.useLocalLine": {
          "type": "boolean",
          "description": "Highlight the local line if there's no selection range",
          "default": false
        }
      }
    },
    "commands": [
      {
        "command": "openOnGitHub.openProject",
        "title": "Open on GitHub: Project"
      },
      {
        "command": "openOnGitHub.openFile",
        "title": "Open on GitHub: File"
      },
      {
        "command": "openOnGitHub.openFileHistory",
        "title": "Open on GitHub: File History"
      },
      {
        "command": "openOnGitHub.openFileBlame",
        "title": "Open on GitHub: File Blame"
      },
      {
        "command": "openOnGitHub.openFilePermalink",
        "title": "Open on GitHub: File Permalink"
      },
      {
        "command": "openonGitHub.openIssues",
        "title": "Open on GitHub: Issues"
      },
      {
        "command": "openOnGitHub.openPullRequests",
        "title": "Open on GitHub: Pull Requests"
      },
      {
        "command": "openOnGitHub.openReleases",
        "title": "Open on GitHub: Releases"
      },
      {
        "command": "openOnGitHub.openActions",
        "title": "Open on GitHub: Actions"
      },
      {
        "command": "openOnGitHub.openProjects",
        "title": "Open on GitHub: Projects"
      },
      {
        "command": "openOnGitHub.openWiki",
        "title": "Open on GitHub: Wiki"
      },
      {
        "command": "openOnGitHub.openSettings",
        "title": "Open on GitHub: Settings"
      },
      {
        "command": "openOnGitHub.copyFilePermalink",
        "title": "Open on GitHub: Copy File Permalink"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf out && webpack --mode production",
    "publish": "vsce publish --no-dependencies && pnpm publish",
    "compile": "webpack --mode development",
    "compile:watch": "webpack --mode development --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "engines": {
    "vscode": "^1.31.0"
  },
  "dependencies": {
    "@types/node": "^10.12.8",
    "absolute": "0.0.1",
    "find-up": "^2.1.0",
    "json5": "^0.5.1",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "pify": "^3.0.0",
    "simple-git": "^2.37.0"
  },
  "devDependencies": {
    "ts-loader": "^5.2.1",
    "typescript": "~2.4.1",
    "vscode": "^1.1.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "files": [],
  "keywords": [
    "vscode",
    "extension",
    "open",
    "in",
    "github"
  ],
  "categories": [
    "Other"
  ]
}
